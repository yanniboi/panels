/**
 * @file
 */

/**
 * Implements hook_block_view_alter().
 *
 * Add contextual links to edit mini panels.
 */
function panels_mini_block_view_alter(&$build, $block) {
  if ($block->getBaseId() == 'mini_panel') {
    $build['#contextual_links']['mini_panel'] = [
      'route_parameters' => ['mini_panel' => $block->getDerivativeId()],
    ];
  }
}
